    "prettier": "3.5.0",
    "prettier-plugin-gherkin": "3.1.1",
    "prettier-plugin-java": "2.6.7",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.8",
    "sinon": "19.0.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0",
    "vite": "6.2.1",
