    "piqure": "2.1.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@prettier/plugin-xml": "3.4.1",
    "@types/sinon": "17.0.4",
